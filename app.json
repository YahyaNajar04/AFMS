{
  "expo": {
    "name": "AFMS",
    "slug": "FYP_Finance_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LOGO.png",
    "scheme": "FYP_Finance_App_scheme",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2ECC71"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.AFMS.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ec7580e-ee3a-42b6-9e7a-a2d238537cf5"
      }
    }
  }
}
